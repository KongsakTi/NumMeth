      "2\n"
     ]
    }
   ],
   "source": [
    "def getNumT(e):\n",
    "    n = 0\n",
    "    while not (calError(n) <= e):\n",
    "        n += 2\n",
    "    return (n/2)\n",
    "print getNumT(10**(-7))"
